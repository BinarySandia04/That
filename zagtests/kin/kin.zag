get 'std.io

kin Entity {
  @ estatic: Int // @ vol dir estatic
  $ maxHealth: Int // $ vol dir privat
  $ health: Num
  hola: Str
  $ damage: Num
  $ defense: Int
  position: Num
  $ alive: Bul

  fn Debug {
    std.println("Test: " + hola)
  }
}

a: Entity
b: Entity
c: Entity
p: Arr<Entity> = [a,b,c]

lup i | p.size() {
  p[i].hola = "Miau numero " + i
  p[i].Debug()
}



